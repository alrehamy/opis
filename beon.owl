        <rdfs:domain rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralAgent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralAgent"/>
        <rdfs:comment xml:lang="en">A relation between two Natural Persons participating in a same Behavior; e.g., &apos;Vitas and Jimmy are talking to each other&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">interacts with</rdfs:label>
    </ObjectProperty>
    


