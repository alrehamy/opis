                <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#includesObject"/>
                <owl:allValuesFrom rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Object"/>
