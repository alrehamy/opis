            </Restriction>
        </rdfs:subClassOf>
    </Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#EnvironmentalCoverage -->

    <Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#EnvironmentalCoverage">
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalPlace"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralEntity"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isCoverageAreaFor"/>
                <allValuesFrom>
                    <Class>
                        <intersectionOf rdf:parseType="Collection">
