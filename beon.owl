    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#assumes -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#assumes">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#conceptualizes"/>
        <inverseOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isAssumedBy"/>
        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief"/>
        <rdfs:comment xml:lang="en">Relation according to which a Natural Person assumes a Concept or a Description that represent a premisse needed to conceive a mental model, mapping, hypothesis or rational.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#believesIn -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#believesIn">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#conceptualizes"/>
        <inverseOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isBelievedBy"/>
        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief"/>
        <rdfs:comment xml:lang="en">Relation according to which a Natural Person believes that a Concept or a Description is stated as his/her personal beliefs. The main difference between &apos;Believes In&apos; and &apos;Assumes&apos; is that this latter involve premisses and hypothesis to ratione about something, while the first personally states as true or acceptable according a set of personal values.</rdfs:comment>
        <rdfs:label xml:lang="en">believes in</rdfs:label>
    </ObjectProperty>
    


