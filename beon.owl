        <rdfs:domain rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralGoal"/>
        <rdfs:label xml:lang="en">contributes to</rdfs:label>
    </ObjectProperty>
    


