    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#SemanticPerceptionDatatype -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#SemanticPerceptionDatatype">
        <rdfs:subClassOf rdf:resource="http://kt.ijs.si/panovp/OntoDT#OntoDT_598925"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.obofoundry.org/ro/ro.owl#has_part"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#SemanticPerceptionValueSpace"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">semantic perception datatype</rdfs:label>
