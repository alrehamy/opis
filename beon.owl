    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralGoal -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralGoal">
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Goal"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralEntity"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isConceptualizedBy"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralAgent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isAssumedBy"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralAgent"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The &apos;Description&apos; of a &apos;Situation&apos; that is desired by a &apos;Behavioral Agent&apos;.
The desired &apos;Situation&apos; (&apos;Behavioral Goal&apos;) is normally associated with a &apos;Behavioral Plan&apos; that describes how to achieve it. The main difference between &apos;Behavioral Goal&apos; and its superclass &apos;Goal&apos; (DUL) is the association of at least one &apos;Behavioral Agent&apos; who conceptualize it.
		</rdfs:comment>
        <rdfs:label xml:lang="en">Behavior goal</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief">
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Concept"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralEntity"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isAssumedBy"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralAgent"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isBelievedBy"/>
