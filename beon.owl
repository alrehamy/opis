        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief"/>
        <rdfs:comment xml:lang="en">Relation according to which a Natural Person is sure about a Concept or a Description that represent a concept conceived and used in a mental model, mapping, hypothesis or rational.</rdfs:comment>
        <rdfs:label xml:lang="en">knows about</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#observedFromProperty -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#observedFromProperty">
        <rdfs:subPropertyOf rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#observedProperty"/>
        <rdfs:label xml:lang="en">observed from property</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#observesFrom -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#observesFrom">
        <rdfs:label xml:lang="en">observes from</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#onCoverageAreaOf -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#onCoverageAreaOf">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#hasLocation"/>
        <rdfs:domain>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalObject"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralEntity"/>
                </intersectionOf>
            </Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#EnvironmentalCoverage"/>
        <rdfs:label xml:lang="en">Relation to describe that a &apos;Behavioral Entity&apos; of the type &apos;Physical Object&apos; (e.g., a car) is inside of coverage area (DUL:&apos;Spatial Region&apos;) defined in a &apos;Environmental Coverage&apos;.</rdfs:label>
        <rdfs:label xml:lang="en">on coverage area of</rdfs:label>
    </ObjectProperty>
    


