    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isCoverageAreaFor -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isCoverageAreaFor">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isLocationOf"/>
        <inverseOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#onCoverageAreaOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#EnvironmentalCoverage"/>
        <rdfs:range>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalObject"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#BehavioralEntity"/>
                </intersectionOf>
            </Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A relation of coverage holded by &apos;Environmental Coverage Area&apos; and any physical &apos;Behavioral Entity&apos; inside that coverage. 
For example, the &apos;Environmental Coverage Area&apos; of a parking area &apos;is coverage area for&apos; a car (&apos;Actionable Object&apos;) and a person (&apos;Behavioral Agent&apos;).</rdfs:comment>
        <rdfs:label xml:lang="en">is coverage area for</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isKnownBy -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isKnownBy">
