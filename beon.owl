        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Agent"/>
        <rdfs:label xml:lang="en">is known by</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isPropertyOfBehavioralEntity -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#isPropertyOfBehavioralEntity">
