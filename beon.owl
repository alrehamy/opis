        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#SpaceRegion"/>
        <rdfs:comment xml:lang="en">Property of &apos;Physical Object&apos; for its Spatial Coverage Area.</rdfs:comment>
        <rdfs:label xml:lang="en">has spatial boundaries</rdfs:label>
    </ObjectProperty>
    


