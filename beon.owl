        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Agent"/>
        <rdfs:comment xml:lang="en">Relation that describes the use of a Physical Object by a Agent</rdfs:comment>
        <rdfs:label xml:lang="en">is used by</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#knowsAbout -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#knowsAbout">
