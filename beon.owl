    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#perceives">
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
        <rdfs:label xml:lang="en">perceives</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#perceivesFrom -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#perceivesFrom">
        <obo:IAO_0000115 xml:lang="en">Relation between a &apos;Virtual Sensor&apos; and a &apos;Feature of Interest&apos; which Properties are observed in order to perceive higher-level semantic</obo:IAO_0000115>
        <rdfs:label xml:lang="en">perceives from</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2015/BeOn#reasonsAbout -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#reasonsAbout">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#conceptualizes"/>
        <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thiago/ontologies/2015/BeOn#Belief"/>
        <rdfs:comment xml:lang="en">Relation according to which a Natural Person reasons about a Concept or a Description. This relation is different from other conceptualization, because it does not assume as true or not, indeed it conceive the Concept or Description in a possibility degree.</rdfs:comment>
        <rdfs:label xml:lang="en">reasons about</rdfs:label>
